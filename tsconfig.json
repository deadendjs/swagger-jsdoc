{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node18",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true,
    "outDir": "dist",
    "types": ["jest", "node"]
  }
}
