{
  "name": "swagger-jsdoc",
  "description": "Generates swagger doc based on JSDoc",
  "version": "8.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node examples/app/app.js",
    "lint": "eslint .",
    "test:lint": "eslint .",
    "test:js": "jest --verbose",
    "test": "npm run test:lint && npm run test:js"
  },
  "main": "index.js",
  "bin": {
    "swagger-jsdoc": "./bin/swagger-jsdoc.js"
  },
  "dependencies": {
    "commander": "6.2.0",
    "doctrine": "3.0.0",
    "glob": "^10.4.5",
    "lodash.mergewith": "^4.6.2",
    "swagger-parser": "^10.0.3",
    "yaml": "2.0.0-1"
  },
  "devDependencies": {
    "eslint": "8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-webpack-plugin": "^4.0.1",
    "express": "4.21.2",
    "husky": "7.0.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "supertest": "6.3.4"
  },
  "license": "MIT",
  "homepage": "https://github.com/deadendjs/swagger-jsdoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/deadendjs/swagger-jsdoc.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "jsdoc"
  ],
  "author": "https://github.com/deadendjs/swagger-jsdoc/graphs/contributors",
  "bugs": {
    "url": "https://github.com/deadendjs/swagger-jsdoc/issues"
  },
  "resolutions": {
    "minimist": ">=1.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
