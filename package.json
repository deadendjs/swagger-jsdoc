{
  "name": "@deadendjs/swagger-jsdoc",
  "description": "Generates swagger doc based on JSDoc",
  "version": "8.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node examples/app/app.js",
    "lint": "eslint .",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test": "npm run lint && npm run test:unit",
    "prepare": "husky"
  },
  "main": "index.js",
  "bin": {
    "swagger-jsdoc": "./bin/swagger-jsdoc.js"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "10.1.0",
    "commander": "^14.0.0",
    "doctrine": "3.0.0",
    "glob": "^11.0.2",
    "lodash.mergewith": "^4.6.2",
    "yaml": "2.0.0-1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^29.0.1",
    "express": "4.21.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1"
  },
  "license": "MIT",
  "homepage": "https://github.com/deadendjs/swagger-jsdoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/deadendjs/swagger-jsdoc.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "jsdoc"
  ],
  "author": "https://github.com/deadendjs/swagger-jsdoc/graphs/contributors",
  "bugs": {
    "url": "https://github.com/deadendjs/swagger-jsdoc/issues"
  },
  "resolutions": {
    "minimist": ">=1.2.3"
  },
  "lint-staged": {
    "*.{js,json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
